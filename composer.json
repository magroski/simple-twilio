{
    "name": "magroski/simple-twilio",
    "description": "A simple client to send messages using Twilio",
    "type": "library",
    "keywords": [
        "Twilio",
        "SMS"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Magroski",
            "email": "magroski@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SimpleTwilio\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "twilio/sdk": "^5.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.0",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.1",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "4.5",
        "squizlabs/php_codesniffer": "*"
    }
}
